{
  "extends": ["airbnb-standard"],
  "rules": {
    "strict": 0,
    "no-restricted-syntax": [
      "error",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-restricted-properties": [0, {
      "property": "pow"
    }]
  },
  "globals": {
    "describe": true,
    "it": true,
    "expect": true
  }
}
